{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:43300/",
      //#if (HttpsEverywhere)
      "sslPort": 44300
      ////#else
      //"sslPort": 0
      //#endif
    }
  },
  "profiles": {
    //#if (Docker)
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "publishAllPorts": true,
      //#if (HttpsEverywhere)
      "useSSL": true
      ////#else
      //"useSSL": false
      //#endif
    },
    //#endif
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "IIS Express Production": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
    },
    //#if (IIS)
    // Run the application using full IIS using its development time support. This requires some setup, see:
    // https://blogs.msdn.microsoft.com/webdev/2017/07/13/development-time-iis-support-for-asp-net-core-applications/
    "IIS (Setup Required)": {
      "commandName": "IIS",
      "launchBrowser": true,
      "launchUrl": "",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "IIS Production (Setup Required)": {
      "commandName": "IIS",
      "launchBrowser": true,
      "launchUrl": "",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
    },
    //#endif
    "Kestrel": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "",
      //#if (HttpsEverywhere)
      "applicationUrl": "https://localhost:5001;http://localhost:5000",
      ////#else
      //"applicationUrl": "http:////localhost:5000",
      //#endif
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Kestrel Production": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "",
      //#if (HttpsEverywhere)
      "applicationUrl": "https://localhost:5001;http://localhost:5000",
      ////#else
      //"applicationUrl": "http:////localhost:5000",
      //#endif
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
    },
    // dotnet watch run must be run without the Visual Studio debugger using CTRL+F5.
    "Watch Kestrel (CTRL+F5)": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "workingDirectory": "$(ProjectDir)",
      "commandLineArgs": "watch run",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Watch Kestrel Production (CTRL+F5)": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "workingDirectory": "$(ProjectDir)",
      "commandLineArgs": "watch run",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
    }
  }
}
